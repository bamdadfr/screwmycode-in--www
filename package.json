        "eslint-plugin-testing-library": "^4.9.0",
        "jest": "^27.0.6",
        "jest-transform-stub": "^2.0.0",
        "react-test-renderer": "^17.0.2",
        "rimraf": "^3.0.2",
        "sass": "^1.35.2",
        "sass-reset": "^1.0.2"
    }
