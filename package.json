  "license": "MIT",
  "author": {
    "name": "Bamdad Sabbagh",
    "email": "hello@bamdadsabbagh.com",
    "url": "https://bamdadsabbagh.com/"
  },
  "scripts": {
    "build": "yarn build:run",
    "build:clean": "rm -rf build",
    "build:dev": "yarn build:clean && yarn build:run",
    "build:run": "GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "package:clean": "rm -rf node_modules",
    "package:reinstall": "yarn package:clean && yarn",
    "package:sort": "npx sort-package-json",
    "start": "BROWSER=none SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@iconify/icons-ant-design": "^1.0.8",
    "@iconify/icons-bi": "^1.0.6",
    "@iconify/icons-mdi": "^1.1.5",
    "@iconify/react": "^1.1.3",
    "detect-browser": "^5.1.1",
    "history": "^5.0.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-keyboard-event-handler": "^1.5.4",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.0.8",
    "recoil": "^0.1.1",
    "three": "^0.123.0",
    "three-orbitcontrols": "^2.102.1"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^1.5.0",
    "@bamdadsabbagh/stylelint-config": "^1.2.161",
    "@types/jest": "^26.0.20",
    "@types/react-router-dom": "^5.1.7",
    "eslint-plugin-prettier": "^3.3.1",
    "react-scripts": "4.0.1",
    "react-test-renderer": "^17.0.1"
  }
}
