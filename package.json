    "scripts": {
        "build": "next build",
        "build:clean": "rimraf build",
        "build:dev": "yarn build:clean && yarn build:run",
        "build:run": "GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
        "cra:dev": "yarn start",
        "cra:eject": "react-scripts eject",
        "cra:start": "BROWSER=none SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "cra:test": "SKIP_PREFLIGHT_CHECK=true react-scripts test",
        "dev": "next dev",
        "export": "next export",
        "lint": "yarn lint:es && yarn lint:style",
        "lint:es": "eslint src-cra/ --ext .js,.jsx,.ts,.tsx --fix",
        "lint:style": "stylelint 'src-cra/**/*.{css,scss,js,jsx,ts,tsx}' --fix",
        "package:clean": "rimraf .next && rimraf node_modules",
        "package:reinstall": "yarn package:clean && yarn",
        "package:sort": "npx sort-package-json",
        "start": "next start"
    },
    "dependencies": {
        "@iconify/icons-ant-design": "^1.0.8",
