        "@sentry/react": "^6.7.1",
        "@sentry/tracing": "^6.7.1",
        "axios": "^0.21.1",
        "detect-browser": "^5.1.1",
        "history": "^5.0.0",
        "howler": "^2.2.1",
        "next": "^11.0.0",
        "next-transpile-modules": "^8.0.0",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-keyboard-event-handler": "^1.5.4",
        "react-toastify": "^7.0.3",
        "recoil": "^0.3.1",
        "speed-to-percentage": "^1.1.2",
        "speed-to-semitones": "^1.2.1",
        "styled-components": "^5.3.0",
        "three": "^0.129.0",
        "three-orbitcontrols": "^2.102.1",
        "ytdl-core": "^4.8.2"
    },
    "devDependencies": {
