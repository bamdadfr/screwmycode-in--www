{
    "name": "screwmycode.in",
    "version": "1.7.5",
    "license": "MIT",
    "author": {
        "name": "Bamdad Sabbagh",
        "email": "hello@bamdadsabbagh.com",
        "url": "https://bamdadsabbagh.com/"
    },
    "scripts": {
        "dev": "yarn start",
        "build": "yarn build:run",
        "build:clean": "rm -rf build",
        "build:dev": "yarn build:clean && yarn build:run",
        "build:run": "GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
        "eject": "react-scripts eject",
        "lint": "yarn lint:es && yarn lint:style",
        "lint:es": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
        "lint:style": "stylelint 'src/**/*.{css,scss,js,jsx,ts,tsx}' --fix",
        "package:clean": "rm -rf node_modules",
        "package:reinstall": "yarn package:clean && yarn",
        "package:sort": "npx sort-package-json",
        "start": "BROWSER=none SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@iconify/icons-ant-design": "^1.0.8",
        "@iconify/icons-bi": "^1.1.3",
        "@iconify/icons-mdi": "^1.1.5",
        "@iconify/react": "^1.1.3",
        "detect-browser": "^5.1.1",
        "history": "^5.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-keyboard-event-handler": "^1.5.4",
        "react-router-dom": "^5.2.0",
        "react-toastify": "^7.0.3",
        "recoil": "^0.1.1",
        "sass": "^1.32.8",
        "sass-reset": "^1.0.2",
        "speed-to-percentage": "^1.0.2",
        "speed-to-semitones": "^1.1.3",
        "three": "^0.126.0",
        "three-orbitcontrols": "^2.102.1"
    },
    "devDependencies": {
        "@bamdadsabbagh/eslint-config": "^1.9.1",
        "@bamdadsabbagh/stylelint-config": "^1.3.0",
        "react-scripts": "4.0.3"
    }
}
