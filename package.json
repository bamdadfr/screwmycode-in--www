    "scripts": {
        "build": "next build && yarn build:post",
        "build:post": "find .next -type f -name \"*.map\" -exec rm {} \\;",
        "dev": "next dev",
        "export": "next export",
        "lint": "eslint src --ext .js --fix",
        "package:clean": "rimraf .next && rimraf node_modules",
        "package:reinstall": "yarn package:clean && yarn",
        "package:sort": "npx sort-package-json",
        "start": "next start",
        "test": "yarn jest",
        "test:cov": "yarn test:coverage",
        "test:coverage": "jest --coverage && codecov",
        "test:experimental": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
        "test:watch": "jest --watch",
        "test:watch:coverage": "jest --watch --coverage",
        "test:watch:all": "jest --watchAll"
    },
    "dependencies": {
        "@iconify/icons-mdi": "^1.1.22",
        "@iconify/react": "^3.0.0",
        "@react-spring/web": "^9.2.4",
        "@sentry/nextjs": "^6.11.0",
        "@sentry/react": "^6.11.0",
        "@sentry/tracing": "^6.11.0",
        "axios": "^0.21.1",
        "can-autoplay": "^3.0.1",
        "jotai": "^1.3.0",
        "next": "^11.1.0",
        "next-transpile-modules": "^8.0.0",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-device-detect": "^1.17.0",
        "react-dom": "^17.0.2",
        "react-howler": "^5.2.0",
        "soundcloud-scraper": "^5.0.1",
        "speed-to-percentage": "^1.1.3",
        "speed-to-semitones": "^1.2.2",
        "styled-components": "^5.3.0",
        "ytdl-core": "^4.9.1"
    },
    "devDependencies": {
        "@bamdadsabbagh/eslint-config": "^1.16.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^7.0.1",
        "@testing-library/user-event": "^13.2.0",
        "@types/react": "^17.0.17",
        "@types/react-dom": "^17.0.9",
        "@types/styled-components": "^5.1.12",
        "babel-jest": "^27.0.6",
        "babel-plugin-styled-components": "^1.13.2",
        "codecov": "^3.8.2",
        "cross-env": "^7.0.3",
        "eslint-config-next": "^11.1.0",
        "eslint-plugin-jest-dom": "^3.9.0",
        "eslint-plugin-testing-library": "^4.9.0",
        "jest": "^27.0.6",
        "rimraf": "^3.0.2",
        "sass": "^1.37.5",
        "sass-reset": "^1.0.2"
    }
