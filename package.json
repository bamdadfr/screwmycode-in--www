    "@bamdadsabbagh/eslint-config": "^1.18.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
