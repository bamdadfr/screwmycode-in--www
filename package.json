    "jotai": "^1.3.0",
    "next": "^12.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-howler": "^5.2.0",
    "soundcloud-scraper": "^5.0.1",
